class A 
{
	static void m1()
	{
		System.out.println("hello");
	}
	void m2()
	{
		System.out.println("hello1");
	}
	void m3()
	{
		System.out.println("hello2");
	}
}
class B extends A
{
	static void m1()
	{
		System.out.println(m1);
	}
	void m2()
	{
		System.out.println(m2);
	}
	void m3()
	{
		System.out.println(m3);
	}
}


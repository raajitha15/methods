class A 
{
	static void m1()
	{
		System.out.println("Am1");
	}
	void m2()
	{
		System.out.println("Am2");
	}
	void m3()
	{
		System.out.println("Am3");
	}
}
	class B extends A
	{
		static void m1()
		{
			System.out.println("B m1");
		}
		void m2()
		{
			System.out.println("Bm2");
		}
		void m4()
		{
			System.out.println("B m4");
		}
	}
	class TestClass1
	{
		public static void main(String[]args)
		{
			A a1=new A();
			a1.m1();
			a1.m2();
			a1.m3();
			a1.m4();
		}
	}
	
